{
  "name": "supertest-jest-node-learning",
  "version": "1.0.0",
  "main": "index.js",
  "author": "savio777",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node-dev --no-notify --exit-child --transpile-only --respawn --ignore-watch node_modules ./src/server.ts",
    "dev": "ts-node-dev --exit-child src/server.ts",
    "test": "jest",
    "tsc-init": "tsc --init",
    "build": "tsc",
    "production": "node ./dist/index.js",
    "start:server": "ts-node --inspect --transpile-only --ignore-watch node_modules ./src/server.ts",
    "start": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules ./src/server.ts",
    "eslint-start": "eslint --init",
    "create:migration": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -d ./src/database/data-source.ts -n",
    "migration:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run -d ./src/database/data-source.ts",
    "migration:show": "ts-node-dev ./node_modules/typeorm/cli.js migration:show -d ./src/database/data-source.ts",
    "migration:revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert -d ./src/database/data-source.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.29.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "uuidv4": "^6.2.13"
  }
}
